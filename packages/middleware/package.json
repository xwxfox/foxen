{
	"name": "@foxen/middleware",
	"version": "1.2.1",
	"description": "Middleware support for Foxen - middleware.ts and proxy.ts handling",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/xwxfox/foxen",
		"directory": "packages/middleware"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "bun build ./src/index.ts --outdir ./dist --target bun && bun run build:types",
		"build:types": "tsc --project tsconfig.build.json",
		"dev": "bun run build --watch",
		"test": "bun test || echo 'No tests found'",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist .turbo"
	},
	"dependencies": {
		"@foxen/core": "workspace:*",
		"@foxen/config": "workspace:*"
	},
	"devDependencies": {
		"@types/bun": "^1.2.0",
		"typescript": "^5.7.0"
	},
	"keywords": [
		"foxen",
		"nextjs",
		"elysia",
		"middleware",
		"proxy"
	],
	"license": "MIT"
}
